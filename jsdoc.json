{
	"source": {
		"include": ["lib", "package.json", "README.md", "app.js"],
		"exclude": ["lib/Graphics/Font.js"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|documentation)"
	},
	"opts": {
		"encoding": "utf8",
		"readme": "./README.md",
		"destination": "documentation/jsdoc/",
		"recurse": true,
		"verbose": true,
		"template": "node_modules/tui-jsdoc-template"
	},
	"templates": {
		"name": "Companion",
		"footerText": " ",
		"logo": {
			"url": "https://bitfocus.io/images/topbar-logowhite.png",
			"width": "147px",
			"height": "24px",
			"link": "https://bitfocus.io"
		},
		"useCollapsibles": true
	}
}
